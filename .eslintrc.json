        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true,
        "codeceptjs/codeceptjs": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0
    },
    "plugins": ["codeceptjs"]
}
